{
    "name": "comsalud360 WS testing",
    "description": "webservices for comsalud360 made with The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen","comsalud360"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "vluzrmos/collective-html": "1.0.9 as 5.1",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "illuminate/mail": "^5.1",
        "vluzrmos/lumen-cors": "2.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
